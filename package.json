{
  "name": "apple-music-tagging-app",
  "version": "1.0.0",
  "description": "Cross-platform app for tagging Apple Music library",
  "scripts": {
    "install-all": "npm run install-backend && npm run install-mobile && npm run install-web",
    "install-backend": "cd backend && npm install",
    "install-mobile": "cd mobile && npm install",
    "install-web": "cd web && npm install",
    "dev-backend": "cd backend && npm run dev",
    "dev-mobile": "cd mobile && npm start",
    "dev-web": "cd web && npm run dev",
    "build-backend": "cd backend && npm run build",
    "build-web": "cd web && npm run build"
  },
  "keywords": ["apple-music", "music", "tagging", "react-native", "react", "nodejs"],
  "author": "Your Name",
  "license": "MIT"
}